    <property name="styleSheet">
     <string notr="true">/* Main QTabWidget Styling */
QTabWidget {
    font: 700 10pt &quot;Roboto&quot;; /* Clean and modern font */
    background-color: #F0F4F7; /* Light metallic gray for widget background */
    color: #D32F2F; /* Red text for labels */
    border-radius: 5px; /* Rounded corners for a polished look */
    padding: 5px; /* Space around the content */
}

/* Pane Styling */
QTabWidget::pane {
    border: none; /* Remove default pane border */
    margin: 5px; /* Ensure pane doesn't overlap with the widget border */
    background-color: transparent; /* Match background for seamless design */
    border: 4px solid #D32F2F; /* Red border for the pane */
}

/* Tab Styling */
QTabBar::tab {
    font: 700 10pt &quot;Roboto&quot;;
    background-color: transparent;
    color: #D32F2F; /* Red text color */
    border: 2px solid transparent; /* Invisible border for tabs */
    padding: 5px 10px; /* Padding for comfortable interaction */
    border-radius: 5px; /* Rounded corners for tabs */
}

QTabBar::tab:selected {
    background-color: #D32F2F; /* Red background for the selected tab */
    color: white; /* White text for contrast */
    border: 2px solid #D32F2F; /* Border with the same red color */
}

QTabBar::tab:hover {
    background-color: #FF6659; /* Lighter red on hover */
    color: #D32F2F; /* Darker red text color */
}

/* GroupBox Styling */
QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center; /* Center the title at the top */
    padding: 0 5px; /* Spacing around the title */
    color: #D32F2F; /* Red title text color */
    font: 700 14pt &quot;Roboto&quot;; /* Clean, modern font */
}

/* Additional adjustments for GroupBox */
QGroupBox {
    border: 2px solid #D32F2F; /* Red border for group boxes */
    border-radius: 5px; /* Rounded corners */
    margin-top: 10px; /* Spacing from the top */
    background-color: #F0F4F7; /* Light metallic gray background */
}

/* Active Tab Styling */
QTabWidget QTabBar::tab:selected {
    background-color: #C2185B; /* Darker red for the active tab */
}
