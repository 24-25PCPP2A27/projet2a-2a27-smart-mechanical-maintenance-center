   <property name="styleSheet">
    <string notr="true">/* Style for the entire QPushButton */
QPushButton {
    background-color: White;  /* Tomato red background */
    border: 2px solid #FF4500;  /* Slightly darker red border */
    border-radius: 15px;        /* Rounded corners */
    color: black;               /* White text color */
    font-size: 16px;            /* Font size */
    padding: 10px 20px;         /* Padding around text */
    min-width: 150px;           /* Minimum width of button */
    max-width: 390px;           /* Maximum width of button */
    text-align: center;         /* Center the text */
    transition: background-color 0.3s ease, transform 0.3s ease; /* Smooth transitions */
}

/* Hover effect */
QPushButton:hover {
    background-color: #FF4500;  /* Darker red when hovered */
    transform: scale(1.05);      /* Slightly grow the button */
}

/* Pressed effect */
QPushButton:pressed {
    background-color: #FF0000;  /* Bright red when pressed */
    transform: scale(0.98);      /* Slightly shrink the button */
}

/* Focus effect */
QPushButton:focus {
    outline: none;  /* Remove the focus outline */
}
</string>
